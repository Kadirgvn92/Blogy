@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    @await Html.PartialAsync("~/Views/LayoutPartials/MemberLayoutPartial/_MemberHeadPartial.cshtml")
</head>
<body>
    <div class="container-scroller">

        @await Html.PartialAsync("~/Views/LayoutPartials/MemberLayoutPartial/_MemberSidebarPartial.cshtml")

        <div class="container-fluid page-body-wrapper">

            @await Html.PartialAsync("~/Views/LayoutPartials/MemberLayoutPartial/_MemberThemePartial.cshtml")

            @await Html.PartialAsync("~/Views/LayoutPartials/MemberLayoutPartial/_MemberNavbarPartial.cshtml")

            <div class="main-panel">

                <div class="content-wrapper pb-0">

                    @RenderBody()

                </div>
